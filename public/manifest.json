{
  "manifest_version": 3,
  "name": "TS Extension Starter",
  "description": "Моё первое расширение для браузераы",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"]
}
