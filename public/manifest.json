{
  "manifest_version": 3,
  "name": "TS Extension Starter",
  "description": "Моё первое расширение для браузераы",
  "version": "0.0.1",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/background/background.js",
    "type": "module"
  },
  "permissions": ["storage", "activeTab"]
}